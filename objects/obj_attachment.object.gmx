            <string>/// End Step
if (!instance_exists(global.player)) instance_destroy();
if (!instance_exists(obj_gun)) instance_destroy();

if (global.loaded)
{
    if (instance_exists(global.player))
    {
        if (instance_exists(obj_gun)) 
        {
            image_angle = obj_gun.image_angle;
            laserX = obj_gun.gunBarrelx2;
            laserY = obj_gun.gunBarrely2;
            var tPos, hPos;
            var hColDis;
            tDis = 5000;
            hDis = 5000;
            tPos[0] = 0;
            tPos[1] = 0;
            hPos[0] = 0;
            hPos[1] = 0;
    
            var tCol = collision_line(laserX,laserY,x,y,obj_char,false,true);
            if (tCol)
            {
                if (tCol.hp &gt; 0) &amp;&amp; (tCol.id != global.player)
                {
                    tPos = rayCast(laserX,laserY,x,y,obj_char);
                    tDis = point_distance(laserX,laserY,tPos[0],tPos[1])
                }
                else tCol = 0;
            }
    
            hCol = collision_line(laserX,laserY,x,y,obj_charhead,false,true);
            if (hCol)
            {
                if (hCol.parent != global.player)
                {
                    hPos = rayCast(laserX,laserY,x,y,obj_charhead);
                    hDis = point_distance(laserX,laserY,hPos[0],hPos[1])
                }
                else hCol = 0;
            }
            if (hCol) &amp;&amp; (tCol)
            {
                if (hDis &lt; tDis)
                {
                    x = hPos[0];
                    y = hPos[1];
                }
                else if (tDis &lt; hDis) 
                {
                    x = tPos[0];
                    y = tPos[1];
                }
            }    
            else if (hCol) &amp;&amp; (!tCol)
            {
                    x = hPos[0];
                    y = hPos[1];
            }
            else if (!hCol) &amp;&amp; (tCol)
            {
                    x = tPos[0];
                    y = tPos[1];
            }
    
            aDir = point_direction(laserX,laserY,x,y);
    
//
        }
//        else draw = false;
    }
}
</string>
